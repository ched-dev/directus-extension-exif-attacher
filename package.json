{
	"name": "directus-extension-exif-attacher",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook",
		"exif"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^9.5.2"
	},
	"bin": {
		"exif-data-models": "bin/index.js"
	},
	"scripts": {
		"help": "directus-extension build --help",
		"build": "directus-extension build",
		"watch": "directus-extension build --watch",
		"create-data-model": "node bin/create-data-model.js",
		"build-hook-from-package": "directus-extension build -f -t hook -i src/index.ts -o ../../extensions/hooks/exif-data-models/index.js"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.5.2",
		"@types/node": "^17.0.21",
		"dotenv": "^16.0.1",
		"inquirer": "^8.2.4",
		"typescript": "^4.6.2"
	}
}
